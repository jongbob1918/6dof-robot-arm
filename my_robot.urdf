<robot name="my_robot">
  <link name="base_link"/>

  <joint name="joint1" type="revolute">
    <parent link="base_link"/>
    <child link="link1"/>
    <origin xyz="0 0 0.05" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="0" upper="3.14159" effort="1.0" velocity="1.0"/>
  </joint>
  <link name="link1"/>
  <joint name="joint2" type="revolute">
    <parent link="link1"/>
    <child link="link2"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="0" upper="3.14159" effort="1.0" velocity="1.0"/>
  </joint>
  <link name="link2"/>
  <joint name="joint3" type="revolute">
    <parent link="link2"/>
    <child link="link3"/>
    <origin xyz="0 0 0.104" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="0" upper="3.14159" effort="1.0" velocity="1.0"/>
  </joint>
  <link name="link3"/>
  <joint name="joint4" type="revolute">
    <parent link="link3"/>
    <child link="link4"/>
    <origin xyz="0 0 0.125" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="0" upper="3.14159" effort="1.0" velocity="1.0"/>
  </joint>
  <link name="link4"/>

  <joint name="joint5" type="revolute">
    <parent link="link4"/>
    <child link="link5"/>
    <origin xyz="0 0 0" rpy="0 0 0"/> <axis xyz="1 0 0"/>
    <limit lower="0" upper="3.14159" effort="1.0" velocity="1.0"/>
  </joint>
  <link name="link5"/>

  <joint name="tcp_joint" type="fixed">
    <parent link="link5"/>
    <child link="tcp"/>
    <origin xyz="0.182 0 0" rpy="0 0 0"/> </joint>
  <link name="tcp"/>
</robot>